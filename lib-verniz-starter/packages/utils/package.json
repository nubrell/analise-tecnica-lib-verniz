{
  "name": "@nubrell/utils",
  "version": "0.1.0",
  "description": "Utility functions",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nubrell/analise-tecnica-lib-verniz.git"
  },
  "keywords": [
    "utils",
    "utilities",
    "cn",
    "classnames",
    "tailwind"
  ],
  "author": "nubrell",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "minimatch": "^10.1.1",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/minimatch": "^6.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5"
  }
}
